{
  "name": "full-stack",
  "version": "1.0.0",
  "description": "boiler plate for node + react + redux",
  "main": "server.js",
  "scripts": {
    "server": "nodemon server/server.js",
    "client": "webpack-dev-server --config webpack.dev.js --hot ",
    "dev": "concurrently 'npm run server' 'npm run client'",
    "build": "webpack --config webpack.prod.js",
    "start": "npm run build && node server/server.js"
  },
  "keywords": [
    "webpack",
    "babel",
    "node.js",
    "express.js",
    "react.js",
    "redux"
  ],
  "author": "Alexander Antoschuk",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^4.0.1",
    "css-loader": "^1.0.0",
    "eslint": "^5.7.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.18.4",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "dotenv": "^6.1.0",
    "express": "^4.16.3",
    "mongoose": "^5.3.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-transition-group": "^2.5.0",
    "reactstrap": "^6.5.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  }
}
